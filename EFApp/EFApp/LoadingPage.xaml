        <Image x:Name="logopic" Source="http://vignette2.wikia.nocookie.net/clubpenguin/images/0/0e/Secret_Agent_PSA_Postcard.png/revision/latest?cb=20130116025904"
                HeightRequest="160" WidthRequest="160"/>
